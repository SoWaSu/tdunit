.head 5 -  Number: nOperator
.head 5 -  Number: nSum
.head 4 +  Actions
.head 5 -  Set nOut = 0
.head 5 +  If ( sIn = '' )
.head 6 -  Return TRUE
.head 5 -  Set nOperator = 1
.head 5 +  Loop
.head 6 -  Set nChar = SalStrLop( sIn )
.head 6 +  If ( nChar = 0 )
.head 7 +  If ( nStatus = Status.Digit )
.head 8 -  Set nSum = nSum + nOperator * nNumber
.head 8 -  Set nNumber = 0
.head 7 -  Break
.head 6 -  Set sChar = SalNumberToChar( nChar )
.head 6 +  If ( IsDigit(sChar) )
.head 7 -  Set nNumber = nNumber * 10 + nChar - 48
.head 7 -  Set nStatus = Status.Digit
.head 6 +  Else If ( IsWhitespace( sChar ) )
.head 7 -  ! do nothing
.head 6 +  Else If ( IsOperator( sChar ) )
.head 7 +  If ( nStatus = Status.Digit )
.head 8 -  Set nSum = nSum + nOperator * nNumber
.head 8 -  Set nNumber = 0
.head 7 +  Else If (nStatus = Status.Operator )
.head 8 -  Return FALSE	! more than one operator is not allowed
.head 7 +  If ( sChar = '-' )
.head 8 -  Set nOperator = -1
.head 7 +  Else
.head 8 -  Set nOperator = 1
.head 7 -  Set nStatus = Status.Operator
.head 6 +  Else
.head 7 -  Set nOut = 0
.head 7 -  Return FALSE
.head 5 -  Set nOut = nSum
.head 5 -  Return TRUE
